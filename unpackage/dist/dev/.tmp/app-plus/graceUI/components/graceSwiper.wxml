<view><swiper class="grace-swiper" style="{{'height:'+(swiperHeight+'upx')+';'}}" autoplay="true" indicator-dots="{{indicatorDots}}" indicator-color="rgba(255, 255, 255, 1)" indicator-active-color="#FF1212" interval="{{interval}}"><block wx:for="{{items}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><navigator url="{{item.path}}" open-type="{{item.openType}}"><image src="{{item.imgUrl}}" id="{{swiperId+'-item-1'}}" mode="scaleToFill"></image><block wx:if="{{!indicatorDots}}"><view class="title">{{item.title}}</view></block></navigator></swiper-item></block></swiper></view>