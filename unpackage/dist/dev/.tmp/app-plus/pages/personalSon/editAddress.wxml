<view class="index_class"><view class="edit-address"><form class="new_from" style="display:block;" action><view class="form_list _div"><label class="_span">收货人</label><input type="text" placeholder="请填写收货人姓名" data-event-opts="{{[['input',[['__set_model',['$0','contacts','$event',[]],['addresinfo']]]]]}}" value="{{addresinfo.contacts}}" bindinput="__e"/></view><view class="form_list _div"><label class="_span">联系电话</label><input type="number" placeholder="请填写联系电话" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['addresinfo']]]]]}}" value="{{addresinfo.phone}}" bindinput="__e"/></view><view class="form_list _div"><label class="_span">所在省</label><picker mode="selector" range-key="pro_name" range="{{pirkCity.prores}}" data-event-opts="{{[['change',[['bindPickerChange',[1,'$event']]]]]}}" bindchange="__e"><input style="width:100%;" type="text" disabled="true" placeholder="请选择所在省" value="{{pro_name.pro_name}}"/></picker></view><view class="form_list _div"><label class="_span">所在市</label><picker mode="selector" range-key="city_name" disabled="{{disabled==1?false:true}}" range="{{pirkCity.cityres}}" data-event-opts="{{[['tap',[['isdisabled',['$event']]]],['change',[['bindPickerChange',[2,'$event']]]]]}}" bindtap="__e" bindchange="__e"><input style="width:100%;" type="text" disabled="true" placeholder="请选择所在市" value="{{city_name.city_name}}"/></picker></view><view class="form_list _div"><label class="_span">所在区</label><picker range-key="area_name" disabled="{{disabled2==1?false:true}}" range="{{pirkCity.areares}}" data-event-opts="{{[['tap',[['isdisabled',['$event']]]],['change',[['bindPickerChange',[3,'$event']]]]]}}" bindtap="__e" bindchange="__e"><input style="width:100%;" type="text" disabled="true" placeholder="请选择所在区" value="{{area_name.area_name}}"/></picker></view><view class="form_list _div"><label class="_span">详细地址</label><input type="text" placeholder="街道,楼牌号等" data-event-opts="{{[['input',[['__set_model',['$0','address','$event',[]],['addresinfo']]]]]}}" value="{{addresinfo.address}}" bindinput="__e"/></view><view class="form_list _div"><label class="_span">设置默认地址</label><switch checked="{{addresinfo.moren}}" data-event-opts="{{[['change',[['switch2Change',['$event']]]]]}}" bindchange="__e"></switch></view></form><view class="add_btn"><button data-event-opts="{{[['tap',[['NewAddress',['$event']]]]]}}" class="cash_btn" bindtap="__e">保存地址</button></view></view></view>