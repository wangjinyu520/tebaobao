<view class="index_class"><view class="setup"><view class="per-lists" style="margin-top:0;"><view data-event-opts="{{[['tap',[['upDate',['$event']]]]]}}" class="items title" bindtap="__e"><view>头像</view><image mode="aspectFill" src="{{userinfo.headimgurl?userinfo.headimgurl:'../../static/images/img/timg.png'}}"></image></view></view><view class="per-lists"><view class="items">用户名<view style="width:80%;"><input type="text" placeholder="请输入用户名" data-event-opts="{{[['input',[['__set_model',['$0','user_name','$event',[]],['userinfo']]]]]}}" value="{{userinfo.user_name}}" bindinput="__e"/><view class="iconfont icon_e60c _i"></view></view></view><view class="items">性别<view style="width:80%;"><input style="flex:1;" type="text" disabled="true" data-event-opts="{{[['tap',[['actionSheetTap',['$event']]]]]}}" value="{{array[index]}}" bindtap="__e"/><view class="iconfont icon_e60c _i"></view></view></view><view class="items">生日<view style="width:80%;"><picker style="flex:1;" mode="date" value="{{date}}" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{date}}</view></picker><view class="iconfont icon_e60c _i"></view></view></view><view class="items">邮箱<view style="width:80%;"><input type="text" placeholder="请输入邮箱" data-event-opts="{{[['input',[['__set_model',['$0','email','$event',[]],['userinfo']]]]]}}" value="{{userinfo.email}}" bindinput="__e"/><view class="iconfont icon_e60c _i"></view></view></view></view><view class="person-button"><button class="btn" type="warn" data-event-opts="{{[['tap',[['confirmEdit',['$event']]]]]}}" bindtap="__e">确认修改</button></view></view></view>