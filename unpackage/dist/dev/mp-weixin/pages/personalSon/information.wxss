.per-lists {
  margin: 0!important;
}
.per-lists picker {
  width: 100%;
}
.per-lists picker input {
  width: 100%;
  color: #333;
}
.per-lists .infoImg {
  width: 80rpx!important;
  height: 80rpx!important;
  border-radius: 10rpx;
}
.per-lists .items-images {
  padding: 0 30rpx 30rpx 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.per-lists .items-images image {
  width: 49%!important;
  height: 215rpx!important;
}
.filephoto {
  background-color: #fff;
  padding-bottom: 45rpx;
}
.filephoto .cation-info {
  background-color: #fff;
}
.filephoto image {
  display: block;
  width: 344rpx;
  height: 203rpx;
  margin: 0 auto;
}
.filephoto .num {
  text-align: center;
  color: #999999;
  font-size: 30rpx;
  margin-top: 20rpx;
}
.person-button {
  padding-bottom: 20rpx!important;
}
.person-button .btn-text {
  text-align: left!important;
}
.scroll-Y {
  height: 500rpx;
}
.uni-list-cell-pd {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 90rpx;
  line-height: 90rpx;
  padding-left: 30rpx;
}
.checkbox-3 {
  width: 100%;
  text-align: left;
}

